<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.project.sikdorock.repository.OrderNumDAO">

    <select id="getOrderSeq" resultType="Integer">
        select seqOrderNumber.nextVal from dual
    </select>

    <select id="getSubSeq" resultType="Integer">
        select seqSubNumber.nextVal from dual
    </select>
</mapper>